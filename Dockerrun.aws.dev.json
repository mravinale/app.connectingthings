{
  "AWSEBDockerrunVersion": 2,
  "authentication": {
    "bucket": "elasticbeanstalk-us-east-1-085850360071",
    "key": "config.json"
  },
  "volumes": [
    {
      "name": "node-app",
      "host": {
        "sourcePath": "/var/app/current/node-app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "connectingthings/app:dev",
      "essential": true,
      "memory": 500,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        },
        {
          "hostPort": 443,
          "containerPort": 3000
        },
        {
          "hostPort": 3001,
          "containerPort": 3001
        },
        {
          "hostPort": 1884,
          "containerPort": 1884
        },
        {
          "hostPort": 1883,
          "containerPort": 1883
        }
      ],
      "command": [
      ],
      "mountPoints": [
      ]
    }
  ]
}
