{
  "AWSEBDockerrunVersion": 2,
  "Authentication": {
    "Bucket": "elasticbeanstalk-us-est-1-085850360071",
    "Key": "docker/config.json"
  },
  "volumes": [
    {
      "name": "node-app",
      "host": {
        "sourcePath": "/var/app/current/node-app"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "node-app",
      "image": "connectingthings/app:dev",
      "essential": true,
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        },
        {
        "hostPort": 3001,
        "containerPort": 3001
        },
        {
        "hostPort": 1884,
        "containerPort": 1884
        },
        {
        "hostPort": 1883,
        "containerPort": 1883
        }
      ],
      "command": [
      ],
      "mountPoints": [
        {
        "sourceVolume": "node-app",
        "containerPath": "/var/www/node"
        }
      ]
    }
  ]
}